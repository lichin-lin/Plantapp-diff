{
  "manifest_version": 2,
  "name": "Diff.Plantapp",
  "version": "0.1.6",
  "description": "A handy diff plugin for your version control service",
  "author": "Lichin Lin",
  "icons": {
    "16": "/icon16.png",
    "48": "/icon48.png",
    "128": "/icon128.png"
  },
  "browser_action": {
    "default_icon": "/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://projects.plantapp.io/p/*"],
      "css": ["/static/css/main.css"],
      "js": ["/static/js/main.js"]
    }
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
