{
  "manifest_version": 2,
  "name": "Plantapp Diff Tool",
  "version": "0.1.0",
  "description": "upgrade your version control service with better diff function",
  "author": "Lichin-Lin",
  "icons": {
    "16": "/icon16.png",
    "32": "/icon32.png",
    "48": "/icon48.png"
  },
  "content_scripts": [
    {
      "matches": ["https://projects.plantapp.io/p/*"],
      "css": ["/static/css/main.css"],
      "js": ["/static/js/main.js"]
    }
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'"
}
